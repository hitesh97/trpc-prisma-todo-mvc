{
  "name": "@examples/trpc-next-prisma-todomvc",
  "version": "10.4.3",
  "private": true,
  "scripts": {
    "migrate-sqlite": "prisma migrate dev --name init --schema=./prisma/_sqlite/schema.prisma",
    "migrate-postgres": "prisma migrate dev",
    "generate-sqlite": "prisma generate --schema=./prisma/_sqlite/schema.prisma",
    "generate-postgres": "prisma generate",
    "prisma-studio-sqlite": "prisma studio --schema=./prisma/_sqlite/schema.prisma",
    "prisma-studio-postgres": "prisma studio",
    "db:generate:all": "run-s migrate-sqlite generate-sqlite",
    "dx:next": "run-s migrate-sqlite generate-sqlite && next dev",
    "dx:prisma-studio": "pnpm prisma-studio-sqlite",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "build-sqlite": "run-s generate-sqlite migrate-sqlite && next build",
    "build-postgres": "run-s generate-postgres migrate && next build",
    "build": "pnpm build-postgres",
    "start": "next start",
    "studio": "prisma studio",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "playwright test",
    "test-dev": "start-server-and-test 'next dev' http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.19.1",
    "@trpc/client": "^10.4.3",
    "@trpc/next": "^10.4.3",
    "@trpc/react-query": "^10.4.3",
    "@trpc/server": "^10.4.3",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nodemailer": "^6.4.6",
    "clsx": "^1.2.1",
    "cookie": "^0.5.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^13.0.6",
    "nodemailer": "^6.8.0",
    "prisma": "^4.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "superjson": "^1.12.0",
    "todomvc-app-css": "^2.4.2",
    "todomvc-common": "^1.0.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@tanstack/react-query-devtools": "^4.19.1",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.15.1",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
