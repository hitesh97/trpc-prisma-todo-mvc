    "@mantine/nprogress": "^5.9.2",
    "@mantine/prism": "^5.9.2",
    "@mantine/spotlight": "^5.9.2",
    "@mantine/tiptap": "^5.9.2",
    "@prisma/client": "^4.7.1",
    "@tabler/icons": "^1.116.1",
